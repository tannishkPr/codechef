#include <iostream>
#include <bits/stdc++.h> 

using namespace std;

int main() {
	int t;
	cin >>t;
	while(t--){
	    int n;
	    cin>>n;
	    int a[100];
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    int found;
	    cin>>found;
	    int element = a[found-1];
	    sort(a,a+n);
	    for(int i=0;i<n;i++){
	        if(a[i] == element)
	            cout<<(i+1)<<endl;
	    }
	}
	return 0;
}
