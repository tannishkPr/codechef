#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int arr[100001];
        map <int,int> ma;
        int n;
        cin>>n;
        int k;
        for(int i=0;i<n;i++){
            cin>>arr[i];
            ma[arr[i]] = {0};
        }
        cout<<ma.size()<<endl;
    }
	return 0;
}
